{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/Zc:wchar_t",
                "/permissive",
                "/GS",
                "/W3",
                "/EHsc",
                "/nologo",
                "/DUNICODE",
                "/D_UNICODE",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${workspaceFolder}/*.cpp",
                "/link",
                "User32.lib",
                "Gdi32.lib",
                "Kernel32.lib",
                "Advapi32.lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}